APP_NAME="Saar Sinistre"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Configuration de la base de données
DB_CONNECTION=mysql
DB_HOST=db
DB_PORT=3306
DB_DATABASE=saarsinistre
DB_USERNAME=saarsinistre_user
DB_PASSWORD=saarsinistre_password

# Configuration Redis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

# Configuration de la session
SESSION_DRIVER=redis
SESSION_LIFETIME=120

# Configuration du cache
CACHE_DRIVER=redis
QUEUE_CONNECTION=redis

# Configuration du mail
MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@saarsinistre.com"
MAIL_FROM_NAME="${APP_NAME}"

# Configuration des fichiers
FILESYSTEM_DISK=local

# Configuration Reverb (WebSockets)
REVERB_APP_ID=saarsinistre
REVERB_APP_KEY=saarsinistre-key
REVERB_APP_SECRET=saarsinistre-secret
REVERB_HOST="localhost"
REVERB_PORT=8080
REVERB_SCHEME=http

# Configuration Pusher
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

# Configuration Vite
VITE_APP_NAME="${APP_NAME}"
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# Configuration Orange SMS (si utilisé)
ORANGE_SMS_CLIENT_ID=
ORANGE_SMS_CLIENT_SECRET=
ORANGE_SMS_SENDER_NAME=


N8N_WEBHOOK_URL=
